    <Style x:Key="BlackTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="Background" Value="{DynamicResource BlackGradientbrush}" />
    </Style>

    <my:ModifiedRowStyle x:Key="modifiedRowStyle">
        <my:ModifiedRowStyle.NotChangedStyle>
            <Style TargetType="telerik:GridViewRow">
                <Setter Property="Background" Value="Transparent" />
            </Style>
        </my:ModifiedRowStyle.NotChangedStyle>
        <my:ModifiedRowStyle.ChangedStyle>
            <Style TargetType="telerik:GridViewRow">
                <Setter Property="Background" Value="Yellow" />
            </Style>
        </my:ModifiedRowStyle.ChangedStyle>
    </my:ModifiedRowStyle>

