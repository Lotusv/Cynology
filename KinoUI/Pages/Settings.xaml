<UserControl
    x:Class="KinoUI.Pages.Settings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mui="http://firstfloorsoftware.com/ModernUI"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid>
        <mui:ModernTab Layout="List" SelectedSource="/Content/SettingsAppearance.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="appearance" Source="/Content/SettingsAppearance.xaml" />
                <mui:Link DisplayName="about" Source="/Content/About.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
