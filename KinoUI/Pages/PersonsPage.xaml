<UserControl
    x:Class="KinoUI.Pages.PersonsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mui="http://firstfloorsoftware.com/ModernUI"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid>
        <Grid.Background>
            <ImageBrush
                ImageSource="images\Dogs.png"
                Opacity="50"
                Stretch="Uniform" />
        </Grid.Background>
        <mui:ModernTab Layout="List" SelectedSource="/Content/PersonUC.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Persons" Source="/Content/PersonUC.xaml" />

                <mui:Link DisplayName="Kennel Clubs" Source="/Content/KennelClubUC.xaml" />



            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
